#include <iostream>
using namespace std;

int main()
{
    int flag1=0, flag2=0, flag3=0, flag4=0;
    int j;
    for(int i = 0; i<10; i++)
    {
        cin >> j;
        switch(j)
        {
            case 1:
                flag1 = 1;
                break;
            case 2:
                flag2 = 1;
                break;
            case 3:
                flag3 = 1;
                break;
            case 4:
                flag4 = 1;
                break;
        }
    }
    cout << 4 - (flag1+flag2+flag3+flag4);
}
